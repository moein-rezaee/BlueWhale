{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "SqlitePath": "data/registry.db"
  },
  "Registry": {
    "RegistryUrl": "http://registry:5000",
    "TimeoutSeconds": 30
  },
  "Jwt": {
    "Secret": "bluewhale-docker-registry-secret-key-2025",
    "Issuer": "bluewhale.registry",
    "Audience": "bluewhale.registry",
    "AccessTokenMinutes": 60,
    "RefreshTokenDays": 7
  },
  "Cache": {
    "DefaultProvider": "Memory",
    "DefaultTtlMinutes": 60
  }
}

